{
   "name": "back-end",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "dependencies": {
      "bcrypt": "^5.0.1",
      "express": "^4.18.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.6",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/express": "^4.17.13",
      "@types/node": "^16.11.10",
      "@types/uuid": "^8.3.4",
      "@typescript-eslint/eslint-plugin": "^5.25.0",
      "eslint": "^8.16.0",
      "eslint-config-standard-with-typescript": "^21.0.1",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-n": "^15.2.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^6.0.0",
      "ts-node": "10.7.0",
      "ts-node-dev": "^1.1.8",
      "typescript": "4.5.2"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "dev": "tsnd --ignore node_modules src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:show": "yarn typeorm migration:show -d src/data-source.ts",
      "migration:run": "yarn typeorm migration:run -d src/data-source.ts",
      "migration:revert": "yarn typeorm migration:revert -d src/data-source.ts",
      "migration:create": "yarn typeorm migration:create"
   }
}